{
  "schemaVersion": 1,
  "id": "stardust",
  "version": "${version}",
  "name": "Stardust",
  "description": "2b2t Utility Addon for Meteor Client.",
  "authors": [
    "Tas [@0xTas] <root@0xTas.dev>"
  ],
  "contact": {
    "repo": "https://github.com/0xTas/stardust"
  },
  "icon": "assets/stardust/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "dev.stardust.Stardust"
    ]
  },
  "mixins": [
    "stardust.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "147,233,190",
    "meteor-client:commit": "${commit}"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
